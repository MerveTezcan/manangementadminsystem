                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a href="layouts-dark-sidebar.html">Kategori Ayarları</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Ürünler Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="Products" asp-action="Index">Ürün Listesi</a></li>
                                        <li><a asp-area="" asp-controller="Products" asp-action="Save">Ürün Ekle</a></li>
                                    </ul>
                                </li>



                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Ürün Renk Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsColor" asp-action="Index">Renk Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsColor" asp-action="Save">Renk Ekle</a></li>

                                    </ul>
                                </li>


                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Proje Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsProjects" asp-action="Index">Proje Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsProjects" asp-action="Save">Proje Ekle</a></li>

                                    </ul>
                                </li>


