                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsCurrency" asp-action="Index">Para Birimi Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsCurrency" asp-action="Save">Para Birimi Ekle</a></li>

                                    </ul>
                                </li>



                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Ölçü Birimi Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsMeasurement" asp-action="Index">Ölçü Birimi Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsMeasurement" asp-action="Save">Ölçü Birimi Ekle</a></li>

                                    </ul>
                                </li>



                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Kdv Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsVat" asp-action="Index">Kdv Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsVat" asp-action="Save">Kdv Ekle</a></li>

                                    </ul>
                                </li>



                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Ağırlık Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsWeight" asp-action="Index">Ağırlık Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsWeight" asp-action="Save">Ağırlık Ekle</a></li>

                                    </ul>
                                </li>

                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Firma Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="Company" asp-action="Index">Firma Listesi</a></li>
                                        <li><a asp-area="" asp-controller="Company" asp-action="Save">Firma Ekle</a></li>

                                    </ul>
                                </li>

                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Bayi Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="CompanyDealer" asp-action="Index">Bayi Listesi</a></li>
                                        <li><a asp-area="" asp-controller="CompanyDealer" asp-action="Save">Bayi Ekle</a></li>

                                    </ul>
                                </li>

                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Şube Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="BranchOffice" asp-action="Index">Şube Listesi</a></li>
                                        <li><a asp-area="" asp-controller="BranchOffice" asp-action="Save">Şube Ekle</a></li>

                                    </ul>
                                </li>



                                <li>
                                    <a href="javascript: void(0);" class="has-arrow">Ürün Resimleri Ayarları</a>
                                    <ul class="sub-menu" aria-expanded="true">
                                        <li><a asp-area="" asp-controller="ProductsImage" asp-action="Index">Resim Listesi</a></li>
                                        <li><a asp-area="" asp-controller="ProductsImage" asp-action="Save">Resim Ekle</a></li>

                                    </ul>
                                </li>


                            </ul>
                        </li>


                    </ul>
                </div>
                <!-- Sidebar -->
            </div>
        </div>
        <!-- Left Sidebar End -->

        <div class="main-content">
            <!-- Start Page-content -->
            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-flex align-items-center justify-content-between">
                                <h4 class="mb-0">Dashboard</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Minible</a></li>
                                        <li class="breadcrumb-item active">Dashboard</li>
                                    </ol>
                                </div>

                            </div>
                        </div>
                    </div>
                    <main role="main">
                        <div class="container-fluid">@RenderBody()</div>
                    </main>
                    <!-- end page title -->
                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

        </div>

        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <script>document.write(new Date().getFullYear())</script> ©  - Gorevcim.WEB - Teknoloji Uzmanım A.Ş.
                    </div>
                    <div class="col-sm-6">
                        <div class="text-sm-end d-none d-sm-block">
                            Web Sitesi Uzmanım <i class="mdi mdi-heart text-danger"></i> <a href="#" target="_blank" class="text-reset">Teknoloji Uzmanım A.Ş.</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

    </div>

    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- JAVASCRIPT -->

    <script src="~/libs/jquery/jquery.min.js"></script>
    <script src="~/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/libs/simplebar/simplebar.min.js"></script>
    <script src="~/libs/node-waves/waves.min.js"></script>
    <script src="~/libs/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/libs/jquery.counterup/jquery.counterup.min.js"></script>

    <!-- apexcharts -->
    <script src="~/libs/apexcharts/apexcharts.min.js"></script>

    <script src="~/js/pages/dashboard.init.js"></script>

    <!-- select 2 plugin -->
    <script src="~/libs/select2/js/select2.min.js"></script>

    <!-- dropzone plugin -->
    <script src="~/libs/dropzone/min/dropzone.min.js"></script>


    <!-- init js -->
    <script src="~/js/pages/ecommerce-add-product.init.js"></script>

    <!-- ckeditor -->
    <script src="~/libs/@@ckeditor/ckeditor5-build-classic/build/ckeditor.js"></script>

    <!--tinymce js-->
    <script src="~/libs/tinymce/tinymce.min.js"></script>

    <!-- init js -->
    <script src="~/js/pages/form-editor.init.js"></script>


    <!-- App js -->
    <script src="~/js/app.js"></script>

    <!-- DataTables -->
    <script src="~/libs/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/libs/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>

    <!-- DataTables Buttons examples -->
    <script src="~/libs/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/libs/datatables.net-buttons-bs4/js/buttons.bootstrap4.min.js"></script>
    <script src="~/libs/jszip/jszip.min.js"></script>
    <script src="~/libs/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/libs/pdfmake/build/vfs_fonts.js"></script>
    <script src="~/libs/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/libs/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/libs/datatables.net-buttons/js/buttons.colVis.min.js"></script>


    <!-- Responsive examples -->
    <script src="~/libs/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/libs/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    <!-- Datatable init js -->
    <script src="~/js/pages/datatables.init.js"></script>
    <!-- Responsive Table js -->
    <script src="~/libs/admin-resources/rwd-table/rwd-table.min.js"></script>

    <script>
        ClassicEditor
            .create(document.querySelector('#classic-editor'))
            .catch(error => {
                console.error(error);
            });
    </script>



    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
